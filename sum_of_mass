#include <iostream>
#include <stdlib.h>
using namespace std;
int main()
{
    int m, n;
    cout << "Enter size of the 1st cell\n";
    cin >> n;
    cout << "Enter size of the 2nd cell\n";
    cin >> m;
    int a[n][m];
    int b[n][m];
    cout << "Enter elements of 1st massif\n";
    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
            cin >> a[i][j];
    cout << "Enter elements of 2nd massif\n";
    for (int i = 0; i < n; i++)
        for (int j = 0; j < m; j++)
        {
            cin >> b[i][j];
            cout << a[i][j] + b[i][j];
        }
    return 0;
}
